[access]
	inheritFrom = All-Projects
[project]
	description = NetVirt
[access "refs/*"]
	owner = group ldap/gerrit-netvirt-committers
	read = group GitHub Replication
[submit]
	action = rebase if necessary
[allowSuperproject "docs"]
	matching = refs/heads/stable/carbon:refs/heads/stable/carbon
[allowSuperproject "releng/autorelease"]
	matching = refs/heads/stable/boron:refs/heads/stable/boron
